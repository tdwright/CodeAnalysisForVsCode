{
  "name": "codeanalysisforvscode",
  "displayName": "CA for VS Code",
  "description": "Manage Code Analysis rulesets in VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ca4vscode.summarize"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "ca4vscode.summarize",
        "title": "Summarize code analysis results"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "fkill": "^5.3.0"
  }
}
